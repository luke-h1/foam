{
  "name": "foam",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "alphabetize": "node scripts/alphabetizePackageJson.js",
    "commit": "cz",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:ios": "npx expo prebuild -p ios",
    "prebuild:android": "npx expo prebuild -p android",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx,.js.jsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js.jsx .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ios android .expo && yarn install",
    "version": "printf 'export const version = \"%s\";\n' \"$(node -p \"require('./package.json').version\")\" > src/version.ts && git add ."
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-native-async-storage/async-storage": "1.18.2",
