{
  "name": "foam",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "main": "index.js",
  "engines": {
    "node": "20.10.0",
    "pnpm": "9.0.6"
  },
  "scripts": {
    "alphabetize": "node scripts/alphabetizePackageJson.js",
    "android": "expo run:android",
    "check:node": "check-node-version --package",
    "clean": "rm -rf ios android .expo node_modules",
    "commit": "cz",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "ios": "expo run:ios",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js.jsx .",
    "prebuild:android": "npx expo prebuild -p android",
    "prebuild:ios": "npx expo prebuild -p ios",
    "preinstall": "pnpm check:node",
    "prepare": "husky",
    "tsc": "tsc --noEmit",
    "start": "expo start",
    "start:proxy": "ts-node-dev -P tsconfig.proxy.json --respawn --transpile-only proxy.ts",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "web": "expo start --web"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@expo/webpack-config": "^19.0.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.10",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.15",
    "@react-navigation/stack": "^6.3.20",
    "@shopify/flash-list": "1.7.1",
    "@tanstack/react-query": "^5.17.10",
    "@twurple/api": "^7.2.0",
    "@twurple/auth": "^7.2.0",
    "async-mutex": "^0.4.1",
    "axios": "^1.7.7",
    "clsx": "^2.1.0",
    "commitlint": "^18.4.4",
    "core-js": "^3.39.0",
    "date-fns": "^4.1.0",
    "emotettv": "^1.0.2",
    "expo": "~52.0.7",
    "expo-auth-session": "~6.0.0",
    "expo-av": "~15.0.1",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.4",
    "expo-font": "~13.0.1",
    "expo-haptics": "14.0.0",
    "expo-image": "~2.0.0",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.2",
    "expo-screen-orientation": "~8.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.11",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.7",
    "express": "^4.21.0",
    "newrelic-react-native-agent": "^1.4.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.1.0",
    "react-native-orientation-locker": "^1.6.0",
    "react-native-pager-view": "6.4.1",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "^15.8.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^5.2.1",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.12.2",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.17.9",
    "@types/react": "~18.3.12",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-native-video": "^5.0.19",
    "@types/tmi.js": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "check-node-version": "^4.2.1",
    "commitizen": "4.3.0",
    "conventional-changelog-angular-all": "1.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "7.2.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "git-cz": "^4.9.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "lerna": "^8.1.8",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.3.3",
    "@types/cors": "^2.8.17",
    "@types/node": "22.10.1",
    "@types/qs": "^6.9.17"
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "suiteName": "App Jest tests",
    "outputName": "junit-app.xml"
  },
  "pnpm": {
    "overrides": {
      "uuid": "3.4.0"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
