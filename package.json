    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.10",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.15",
    "@react-navigation/stack": "^6.3.20",
    "@shopify/flash-list": "1.6.3",
    "@tamagui/animations-moti": "^1.85.0",
    "@tamagui/font-inter": "1.85.0",
    "@tamagui/lucide-icons": "^1.85.0",
    "@tamagui/react-native-media-driver": "1.85.0",
    "@tamagui/shorthands": "1.85.0",
    "@tamagui/themes": "1.85.0",
    "@tanstack/react-query": "^5.17.10",
    "async-mutex": "^0.4.1",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "commitlint": "^18.4.4",
    "dotenv": "^16.3.1",
    "expo": "~49.0.13",
    "expo-app-loading": "^2.1.1",
    "expo-auth-session": "~5.0.2",
    "expo-av": "~13.4.1",
    "expo-clipboard": "~4.3.1",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.12",
    "expo-font": "~11.4.0",
    "expo-haptics": "12.0.1",
    "expo-image": "~1.3.5",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-screen-orientation": "~6.0.6",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.18",
    "lerna": "^8.0.2",
    "newrelic-react-native-agent": "^1.3.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-orientation-locker": "^1.6.0",
    "react-native-pager-view": "6.2.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "^14.1.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-video": "^5.2.1",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.2.2",
    "tamagui": "1.85.0",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@tamagui/babel-plugin": "1.85.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/react": "~18.2.14",
    "@types/react-native-video": "^5.0.19",
    "@types/tmi.js": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "commitizen": "4.3.0",
    "conventional-changelog-angular-all": "1.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.5.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
