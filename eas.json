{
  "cli": {
    "version": ">=16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "bun": "1.2.5"
    },
    "test": {
      "extends": "base",
      "env": {
        "APP_VARIANT": "test"
      },
      "channel": "test"
    },
    "e2e": {
      "extends": "base",
      "withoutCredentials": true,
      "env": {
        "APP_VARIANT": "e2e",
        "MOCK_SERVER_URL": "http://localhost:3001"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "channel": "e2e"
    },
    "e2e-dev": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "e2e",
        "MOCK_SERVER_URL": "http://localhost:3001"
      },
      "ios": {
        "simulator": true
      },
      "channel": "e2e"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "environment": "preview",
      "env": {
        "APP_VARIANT": "preview"
      },
      "channel": "preview"
    },
    "production": {
      "bun": "1.2.5",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "production"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6742071860"
      }
    }
  }
}
